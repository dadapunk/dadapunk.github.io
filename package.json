{
  "name": "personal-site",
  "version": "3.0.1",
  "homepage": "https://www.sebastian-velasquez.dev/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dadapunk/dadapunk.github.io"
  },
  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true rimraf ./build && react-scripts build",
    "predeploy": "npm run build && react-snap",
    "deploy": "gh-pages -d build",
    "start": " DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "lint": "eslint src",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test --watchAll=false",
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "core-js": "^3.22.8",
    "dayjs": "^1.10.4",
    "openssl": "^2.0.0",
    "prop-types": "^15.7.2",
    "raw.macro": "^0.4.2",
    "react": "^17.0.1",
    "react-burger-menu": "^3.0.3",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-helmet-async": "^1.0.7",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "gh-pages": "^3.2.3",
    "source-map-explorer": "^2.5.2"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  }
}
